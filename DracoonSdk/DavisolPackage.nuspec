    <file src="bin\Release\netstandard2.0\Dracoon.Sdk.dll" target="lib\netstandard2.0\Dracoon.Sdk.dll" />
    <file src="bin\Release\netstandard2.0\Dracoon.Sdk.xml" target="lib\netstandard2.0\Dracoon.Sdk.xml" />
    <file src="bin\Release\netstandard2.0\Dracoon.Sdk.pdb" target="lib\netstandard2.0\Dracoon.Sdk.pdb" />
      
